#include <stdio.h>
#include<iostream>
using namespace std;

int main()
{
    int value = 0;
    while (true)
    {
        scanf("%d",&value);
        for(int i = 1; i < 10; i++)
            {
                printf("%d * %d = %d\n",value,i,value*i);
            }
            if(value == 0)
            {
                break;
            }
        
    }
}

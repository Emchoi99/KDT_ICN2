#include <stdio.h>

int main()
{
    int array[9][9];
    
    for(int i = 1; i <= 9; i++)
    {
        for(int j = 1; j <= 9; j++)
        {
            array[i-1][j-1] = i * j;
        }
    }
    
    for(int k = 1; k <= 9; k++)
    {
        for(int l = 1; l <= 9; l++)
        {
            if(array[k-1][l-1])
            printf("%dx%d= %2d  ",k,l,array[k-1][l-1]);
        }
        printf("\n");
    }
    return 0;
}
